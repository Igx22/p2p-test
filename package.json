{
  "name": "test-p2p",
  "version": "0.1",
  "description": "test libp2p with typescript",
  "main": "src/index.ts",
  "module": "ESNext",
  "type": "module",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "dev": "ts-node-dev --respawn --inspect=9229 --transpile-only ./src/app.ts",
    "dev2": "ts-node-dev --transpile-only ./tests/p2p-message.test.ts",
    "heroku-postbuild": "npm run build",
    "start": "nodemon",
    "inspect": "nodemon --inspect src/app.ts",
    "test": "mocha --inspect=9229 -r ts-node/register tests/**/*.test.ts",
    "lint": "npm run lint:js ",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "epns",
    "push",
    "notifications",
    "ios",
    "android"
  ],
  "author": "Harsh Rajat",
  "license": "UNLICENSED",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "@libp2p/bootstrap": "^6.0.0",
    "it-pipe": "^2.0.5"
  },
  "devDependencies": {
    "@chainsafe/libp2p-noise": "^11.0.1",
    "@libp2p/bootstrap": "^6.0.0",
    "@libp2p/mplex": "^7.1.1",
    "@libp2p/tcp": "^6.1.2",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "it-to-buffer": "^3.0.0",
    "jest": "^24.1.0",
    "libp2p": "^0.42.2",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.1",
    "ts-jest": "^24.0.0",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "tslint": "^5.11.0",
    "typescript": "^4.9.5"
  }
}
